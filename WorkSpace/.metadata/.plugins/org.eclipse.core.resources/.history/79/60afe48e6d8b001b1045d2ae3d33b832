package ArrayList;

import java.util.ArrayList;
import java.util.List;

public class ArrayListMainClass {
	public static void main(String[] args) {
		//1.생성
		List<String> list = new ArrayList<String>();
		
		//2.추가
		
		list.add("브레드");
		list.add("짱구");
		list.add("자두");
		
		//3.삭제
		
		list.remove(0);
		list.remove("짱구");
		
		//4.크기
		list.size();
		
	}
}
