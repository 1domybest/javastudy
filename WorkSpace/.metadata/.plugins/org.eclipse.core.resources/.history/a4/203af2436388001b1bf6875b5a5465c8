package quiz05_starcraft;

public class MainClass {
	public static void main(String[] args) {
		Terran [] terrans = new Terran[10];
		terrans[0] = new Marine();
		terrans[1] = new FireBat();
		terrans[2] = new Tank();
		
		Bunker bk = new Bunker();
		
		bk.add(terrans[0]);
		bk.add(terrans[1]);
		bk.add(terrans[2]);
	}
}
