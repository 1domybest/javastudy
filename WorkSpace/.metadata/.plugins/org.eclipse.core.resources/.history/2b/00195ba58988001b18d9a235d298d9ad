package ex07_exception_class;
//예외 클래스는 "직렬화" 를 대비해서 serialversionID 를 생성하라고 항상 경고를 띄웁니다.
@SuppressWarnings("serial")
public class UpDown_Execption extends Exception {
	//예외클래스는 주로 어떤일을 수행하냐면,
	public UpDown_Execption() {
		super("are you dumb stupid or dumb hu?");//예외메시지를 Exception에게 전달하면 됩니다
	}

}
