package quiz02;

import java.util.ArrayList;
import java.util.LinkedList;
import java.util.List;

//1~10000 사이의 값을 가지고 있는 ArrayList 를 2개 만들고,
//모든 요소를 하나씩 삭제합니다
//1.가장 성능이 나쁜 방법
//1.가장 성능이 좋은 방법

public class MainClass {
	public static void main(String[] args) {
		List<Integer> list1 = new ArrayList<Integer>();
		List<Integer> list2 = new ArrayList<Integer>();
		
		
		long start1 = System.nanoTime();
		
		for (int i = 0; i < list1.size(); i++) {
			list1.add(i+1);
			list2.add(i+1);
		}
		long end1 = System.nanoTime();
		System.out.println("나쁜 "+(end1-start1)+"ns");
		
		
		
		
		
		long start2 = System.nanoTime();
		for (int i = 0; i < list2.size(); i++) {
			list2.add(0,i);
		}
		long end2 = System.nanoTime();
		
		System.out.println("나쁜 "+(end2-start2)+"ns");
		
		
		
		long start3 = System.nanoTime();
		for (int i = 0,length = list1.size() ; i < length; i++) {
			list1.add(0,i);
		}
		long end3 = System.nanoTime();
		
		System.out.println("좋은 "+(end3-start3)+"ns");
		
		
		long start4 = System.nanoTime();
		for (int i = 0,length = list2.size() ; i < length; i++) {
			list1.add(1,i);
		}
		long end4 = System.nanoTime();
		System.out.println("좋은 "+(end4-start4)+"ns");
	}
}
