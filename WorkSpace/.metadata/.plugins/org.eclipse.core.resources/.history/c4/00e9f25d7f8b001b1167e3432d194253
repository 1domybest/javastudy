package ex05_list;

import java.util.ArrayList;
import java.util.List;

public class LinkedListMainClass {
	public static void main(String[] args) {
		//장점: 모든 위치에 추가/삭제가 빠르다
		//단점: 순차적인 접근이 느리다.
		
		
		//성능비교
		List<Integer> list1 = new ArrayList<Integer>();
		List<Integer> list2 = new ArrayList<Integer>();
		
		//1. 추가 (인덱스 0 에 데이터 추가)
		long start = System.nanoTime();
		for (int i = 0; i <= 10000; i++) {
			list1.add(0,i); 
		}
		long end = System.nanoTime();
		
		System.out.println("ArrayList 삽입 소요 시간: "+(end-start)+"ns");
		long start1 = System.nanoTime();
		for (int i = 0; i <= 10000; i++) {
			list1.add(1,i); 
		}
		long end1 = System.nanoTime();
		System.out.println("ArrayList 삽입 소요 시간: "+(end1-start1)+"ns");
		
		
		long start3 = System.nanoTime();
		for (int i = 0, length = list1.size(); i <length; i++) {
			list1.get(i);
		}
		long end3= System.nanoTime();
		System.out.println("LinkedList 순회기간: "+ (end3-start3)+"ns");
		
		long start4 = System.nanoTime();
		for (int i = 0, length = list1.size(); i <length; i++) {
			list1.get(i);
		}
		long end4 = System.nanoTime();
		System.out.println("LinkedList 순회기간: "+ (end4-start4)+"ns");
	}
	
}
